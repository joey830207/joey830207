    "from sympy import *\n",
    "\n",
    "R = symbols('R')\n",
    "\n",
    "def func(R,x,y):\n",
    "    return (x/2)**2 + (y-R)**2 - R**2\n",
    "\n",
    "x = 888\n",
    "y = 1459\n",
    "\n",
    "solve(func(R,x,y),R)"
   ]
  }
